    "@types/pouchdb-browser": "^6.1.5",
    "clsx": "^2.1.1",
    "electron-router-dom": "^1.0.5",
    "phosphor-react": "^1.4.1",
    "pouchdb": "^9.0.0",
    "pouchdb-browser": "^9.0.0",
    "react-router-dom": "^7.9.6",
    "tailwindcss": "^4.1.17",
